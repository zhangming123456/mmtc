@font-face {
  font-family: "iconfont";
  src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'), url('iconfont.ttf?t=1524725375576') format('truetype');
  /* iOS 4.1- */
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.iconfont.icon-jiantou:before {
  content: "\e603";
}
.iconfont.icon-address:before {
  content: "\e630";
}
.azm-address_location {
  position: absolute;
  box-sizing: border-box;
  top: 20rpx;
  left: 30rpx;
  max-width: 276rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: rgba(0, 0, 0, 0.38);
  border-radius: 29rpx;
  padding-left: 52rpx;
  padding-right: 45rpx;
  color: #fff;
}
.azm-address_location text {
  color: #fff;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0;
}
.azm-address_location .azm-icon {
  position: absolute;
  top: 50%;
}
.azm-address_location .azm-icon.left {
  left: 26rpx;
  transform: translate(-50%, -50%);
}
.azm-address_location .azm-icon.right {
  right: 0;
  transform: translate(-50%, -50%);
}
.azm-address_location.pr {
  position: relative;
  left: 0;
  top: 0;
}
.azm-address_location.pa {
  position: absolute;
  left: 0;
  top: 0;
}
.azm-address_location.pf {
  position: fixed;
  left: 0;
  top: 0;
}
.azm-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
}
