page {
  width: 100%;
  height: 100%;
  background-color: #f4f4f4;
}
.azm-home-searchResult #azm-header .azm-search {
  background-color: #fff;
  box-sizing: border-box;
  padding: 24rpx 30rpx;
}
.azm-home-searchResult #azm-header .azm-search .azm-search-l {
  flex: 2;
  position: relative;
  box-sizing: border-box;
}
.azm-home-searchResult #azm-header .azm-search .azm-search-l input,
.azm-home-searchResult #azm-header .azm-search .azm-search-l input.input-placeholder {
  background-color: #f4f4f4;
  font-size: 24rpx;
  width: 100%;
  height: 60rpx;
  min-height: 60rpx;
  border-radius: 60rpx;
  box-sizing: border-box;
  padding-left: 50rpx;
}
.azm-home-searchResult #azm-header .azm-search .azm-search-l .azm-icon-search {
  position: absolute;
  top: 50%;
  left: 20rpx;
  transform: translate(0, -50%);
  z-index: 100;
}
.azm-home-searchResult #azm-header .azm-search .azm-search-r {
  width: 130rpx;
}
.azm-home-searchResult #azm-header .azm-search .azm-search-r .azm-btn-search-cancel {
  border: none;
  line-height: 60rpx;
  padding-right: 0;
}
.azm-home-searchResult .azm-content {
  background-color: #fff;
  min-height: 600rpx;
}
.azm-home-searchResult .azm-content scroll-view {
  min-height: 600rpx;
  background-color: #f4f4f4;
}
.azm-home-searchResult .azm-content .azm-refresh {
  position: relative;
  height: 100rpx;
  background-color: #f4f4f4;
}
.azm-home-searchResult .azm-content .azm-refresh .azm-refresh-box {
  position: absolute;
  bottom: 0;
  left: 0;
  box-sizing: border-box;
  text-align: center;
  padding: 24rpx 30rpx;
  font-size: 24rpx;
  width: 100%;
}
.azm-home-searchResult .azm-content #tab0 {
  min-height: 200rpx;
}
.azm-home-searchResult .azm-content #tab0 > .flexBox {
  background-color: #fff;
  position: relative;
  box-sizing: border-box;
  padding: 24rpx 30rpx;
}
.azm-home-searchResult .azm-content #tab0 .azm-tab0-l {
  position: relative;
  flex: 2;
  text-align: left;
}
.azm-home-searchResult .azm-content #tab0 .azm-tab0-l .azm-image-box {
  display: block;
  position: relative;
  width: 100%;
  height: 100%;
}
.azm-home-searchResult .azm-content #tab0 .azm-tab0-r {
  position: relative;
  width: 500rpx;
  box-sizing: border-box;
  padding-left: 24rpx;
  line-height: 1;
}
.azm-home-searchResult .azm-content #tab0 .azm-tab0-r .azm-tab0-title {
  font-size: 32rpx;
  color: #282828;
  margin-bottom: 18rpx;
}
.azm-home-searchResult .azm-content #tab0 .azm-tab0-r .azm-tab0-stars {
  margin-bottom: 16rpx;
}
.azm-home-searchResult .azm-content #tab0 .azm-tab0-r .azm-tab0-stars text {
  font-size: 24rpx;
  color: #999;
}
.azm-home-searchResult .azm-content #tab0 .azm-tab0-r .azm-tab0-label {
  display: block;
  margin-bottom: 20rpx;
}
.azm-home-searchResult .azm-content #tab0 .azm-tab0-r .azm-tab0-location {
  font-size: 24rpx;
  color: #999;
}
.azm-home-searchResult .azm-content #tab1 {
  min-height: 200rpx;
}
.azm-home-searchResult .azm-content #tab2 {
  min-height: 200rpx;
}
